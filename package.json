{
  "name": "farmbox-xmas",
  "description": "Farmbox Christmas 19 Offer",
  "version": "1.0.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy && cp -R ./__sapper__/build ./functions/__sapper__",
    "prebuild": "rm -rf functions/__sapper__/build && mkdir -p functions/__sapper__/build",
    "export": "sapper export --legacy",
    "start": "npm run build && firebase serve",
    "predeploy": "npm run build",
    "deploy": "firebase deploy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "lint": "eslint ./src/** --ext js --ext svelte"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^2.9.0",
    "polka": "^1.0.0-next.11",
    "sirv": "^0.4.0",
    "svelte-calendar": "^1.3.4",
    "svelte-simple-modal": "^0.2.2",
    "sveltestrap": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@ianwalter/eslint-config-svelte": "^2.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "cypress": "^3.8.3",
    "eslint": "^6.8.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.11",
    "svelte": "^3.18.1"
  }
}
